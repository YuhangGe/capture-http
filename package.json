{
  "name": "capture-http",
  "productName": "Capture HTTP(s) app",
  "version": "1.0.0",
  "description": "A simple HTTP(s) traffic capture tool",
  "main": "main.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development ./script/fe.js dev",
    "build": "npm run lint && rm -rf ./dist && ./script/fe.js build",
    "lint": "npm run eslint && npm run lesslint",
    "eslint": "eslint --fix .",
    "lesslint": "stylelint app/**/*.less --fix --syntax less",
    "validate": "npm ls"
  },
  "repository": "https://github.com/YuhangGe/capture-http.git",
  "keywords": [
    "http",
    "https",
    "proxy",
    "traffic",
    "capture"
  ],
  "author": "Yuhang Ge",
  "license": "MIT",
  "devDependencies": {
    "chokidar": "^2.0.0",
    "colors": "^1.1.2",
    "electron-packager": "^10.1.1",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "precommit-hook": "^3.0.0",
    "rollup": "^0.53.4",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-string": "^2.0.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "uglify-es": "^3.3.5",
    "ws": "^4.0.0"
  },
  "dependencies": {
    "electron": "^1.7.10",
    "find-free-port": "^1.1.0",
    "highlight.js": "^9.12.0",
    "ip": "^1.1.5",
    "jshexgrid": "0.0.1",
    "lodash-es": "^4.17.4",
    "moment": "^2.20.1",
    "node-forge": "^0.7.1",
    "office-ui-fabric-react": "^5.39.0",
    "pure-notify": "^1.0.3",
    "qrcode": "^1.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "pre-commit": [
    "lint"
  ]
}
